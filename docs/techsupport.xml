      <table name="view_comment" remarks="" type="VIEW" viewSql="select `techsupport`.`comment`.`issue_id` AS `issue_id`,`techsupport`.`comment`.`id` AS `id`,`techsupport`.`comment`.`author_id` AS `author_id`,`techsupport`.`comment`.`posted_at` AS `posted_at`,`techsupport`.`comment`.`content` AS `content`,`techsupport`.`comment`.`reply_to` AS `reply_to` from `techsupport`.`comment` where (`techsupport`.`comment`.`reply_to` is not null) order by `techsupport`.`comment`.`reply_to`,`techsupport`.`comment`.`posted_at` desc">
