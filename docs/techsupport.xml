      <table name="view_issue" remarks="" type="VIEW" viewSql="select `techsupport`.`issue`.`id` AS `id`,`techsupport`.`issue`.`issued_at` AS `issued_at`,`techsupport`.`issue`.`issued_by` AS `issued_by`,coalesce(`techsupport`.`comment`.`content`,'Description of the issue is missing.') AS `content` from (`techsupport`.`issue` join `techsupport`.`comment` on((`techsupport`.`issue`.`id` = `techsupport`.`comment`.`issue_id`))) where ((`techsupport`.`comment`.`id` = 1) and isnull(`techsupport`.`comment`.`reply_to`))">
         <column autoUpdated="false" digits="0" id="0" name="id" nullable="false" remarks="" size="20" type="BIGINT UNSIGNED"/>
         <column autoUpdated="false" digits="0" id="1" name="issued_at" nullable="false" remarks="" size="19" type="DATETIME"/>
         <column autoUpdated="false" digits="0" id="2" name="issued_by" nullable="false" remarks="" size="5" type="SMALLINT UNSIGNED"/>
         <column autoUpdated="false" defaultValue="" digits="0" id="3" name="content" nullable="false" remarks="" size="300" type="VARCHAR"/>
      </table>
      <table name="view_issue_assigned" remarks="" type="VIEW" viewSql="select `s`.`id` AS `id`,`s`.`issued_at` AS `issued_at`,`s`.`issued_by` AS `issued_by`,`s`.`content` AS `content`,`a`.`assignee_id` AS `assignee_id`,`a`.`assigned_id` AS `assigned_id`,`a`.`assigned_at` AS `assigned_at` from (`techsupport`.`view_issue` `s` join `techsupport`.`issue_assignation` `a` on((`s`.`id` = `a`.`issue_id`))) where ((`a`.`assigned_at` = (select max(`suba`.`assigned_at`) from `techsupport`.`issue_assignation` `suba` where (`a`.`issue_id` = `suba`.`issue_id`))) and (not(`s`.`id` in (select `techsupport`.`issue_closing`.`issue_id` from `techsupport`.`issue_closing`))))">
         <column autoUpdated="false" digits="0" id="0" name="id" nullable="false" remarks="" size="20" type="BIGINT UNSIGNED"/>
         <column autoUpdated="false" digits="0" id="1" name="issued_at" nullable="false" remarks="" size="19" type="DATETIME"/>
